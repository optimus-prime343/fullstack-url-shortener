    "jest": "~29.3",
    "nodemon": "^2.0.20",
    "prettier": "~2.8",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "ts-node": "^10.9.1",
    "tsutils": "~3.21",
    "typescript": "~4.9"
  },
  "scripts": {
    "dev": "nodemon",
